<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Fragment1"
    android:orientation="vertical"
    android:focusableInTouchMode="true">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="15dp" />


    <androidx.appcompat.widget.SearchView
        android:id="@+id/search_View"
        android:layout_width="360dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        app:iconifiedByDefault="false"
        app:queryHint="URL을 입력하세요"
        android:background="@drawable/search_edge"
        android:queryBackground="@null"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="10dp" />

    <LinearLayout
        android:layout_width="360dp"
        android:layout_height="440dp"
        android:layout_gravity="center"
        android:background="@drawable/border"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp" />

        <TextView
            android:layout_width="300dp"
            android:layout_height="50dp"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/rank_edge"
            android:gravity="center_horizontal|center_vertical"
            android:textStyle="bold"
            android:text="랭킹 순위 예시1" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="30dp" />

        <TextView
            android:layout_width="300dp"
            android:layout_height="50dp"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/rank_edge"
            android:gravity="center_horizontal|center_vertical"
            android:textStyle="bold"
            android:text="랭킹 순위 예시2" /><TextView
        android:layout_width="match_parent"
        android:layout_height="30dp" />

        <TextView
            android:layout_width="300dp"
            android:layout_height="50dp"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/rank_edge"
            android:gravity="center_horizontal|center_vertical"
            android:textStyle="bold"
            android:text="랭킹 순위 예시3" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="30dp" />

        <TextView
            android:layout_width="300dp"
            android:layout_height="50dp"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/rank_edge"
            android:gravity="center_horizontal|center_vertical"
            android:textStyle="bold"
            android:text="랭킹 순위 예시4" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="30dp" />

        <TextView
            android:layout_width="300dp"
            android:layout_height="50dp"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/rank_edge"
            android:gravity="center_horizontal|center_vertical"
            android:textStyle="bold"
            android:text="랭킹 순위 예시5" />


    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="15dp" />
</LinearLayout>

